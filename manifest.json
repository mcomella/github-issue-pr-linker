{

    "manifest_version": 2,
    "name": "GitHub Issue PR Linker",
    "version": "0.0.1",

    "description": "",

    "options_ui": {
        "page": "dist/options.html",
        "browser_style": true
    },

    "content_scripts": [{
        "matches": ["*://*.github.com/*"],
        "js": ["dist/content_script.js"]
    }],

    "permissions": [
        "*://api.github.com/*",
        "webRequest",
        "storage"
    ]
}
